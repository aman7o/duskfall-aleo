# Frontend Environment Configuration
# Copy this file to .env.local and fill in your values

# IMPORTANT: Per CLAUDE.md - TESTNET ONLY. No local/demo mode.
# Set to 'false' to connect to Aleo testnet (REQUIRED for production)
NEXT_PUBLIC_ALEO_LOCAL_MODE=false

# Aleo RPC endpoint (default: Provable API)
NEXT_PUBLIC_ALEO_RPC=https://api.explorer.provable.com/v1

# Program ID (must match deployed contract)
NEXT_PUBLIC_PROGRAM_ID=digital_will_v7.aleo
